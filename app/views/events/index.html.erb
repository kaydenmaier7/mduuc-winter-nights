	<h2>April 9th, 2017</h2>
	<ul class="event_item">
		<% @events.where(date: 'April 9, 2017').each do |event| %>
		<% if event.needed_volunteers > 0 %>
		<li>
			<%= link_to event.event_type, event_path(event) %>
		</li>
